- @result.each do |k,v|
  %table.listing{:summary => @title}
    %caption= v[0][:bto_code]
    %thead
      %tr
        - @headings.each do |h|
          %th=h
    %tbody
      - v.each do |w|
        %tr{:class => cycle}
          %td= w[:winter]
          - @months.each do |m|
            %td= w[m]
  %br
%br
%p.textnote
  = "Last updated: #{fmt_date @last_updated, "%d %B %Y"}"


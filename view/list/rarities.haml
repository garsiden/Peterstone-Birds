%table.listing{:summary => @title}
  %caption= @caption
  %thead
    %tr
      - @headings.each do |h|
        %th= h
  %tfoot
    %tr
      %td.left{:colspan => 3}= @footer
  %tbody
    - @ds.each do |row|
      %tr{:class => cycle}
        %td.lnowrap= row.bird.name
        %td.cnowrap= fmt_date row.first_date
        %td.left= row.note
%br
%a{:href=>'#wrapper'} Back to top

